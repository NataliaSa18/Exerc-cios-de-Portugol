Algoritmo "consumo"

Var
   distPec, combGasto, consMedio: real

Inicio

   Escreva ("Distancia percorrida: ")
   Leia (distPec)

   Escreva ("Combustível gasto: ")
   Leia (combGasto)

   consMedio <- distPec/combGasto

   Escreval ("Consumo medio = ", consMedio:3:3)
   
Fimalgoritmo